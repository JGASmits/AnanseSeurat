library(Seurat)
prioritize_conda <- function(lib_tree){
cpath <-  grep('conda', lib_tree, value=TRUE, ignore.case=TRUE)
ifelse(length(cpath) == 0, return(lib_tree), return(rev(c(lib_tree, cpath))))
}
new_tree <- prioritize_conda(lib_tree=.libPaths())
.libPaths(new_tree)
.libPaths()
